{
  "name": "polis-server",
  "license": "AGPL-3.0",
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "start": "tsc && node --gc_interval=100 dist/app.js",
    "start:dev": "nodemon --inspect=0.0.0.0:9229 dist/app.js",
    "serve:prod": "node --gc_interval=100 dist/app.js",
    "serve:prod:dev": "nodemon --gc_interval=100 dist/app.js",
    "format": "prettier --config ./prettier.config.js --no-editorconfig 'src/**/*.{js,ts}' --write",
    "format:check": "prettier --config ./prettier.config.js --no-editorconfig 'src/**/*.{js,ts}' --check",
    "lint": "eslint .",
    "test": "jest --forceExit test",
    "test:watch": "npm test -- --watchAll"
  },
  "dependencies": {
    "@google-cloud/translate": "~1.0.0",
    "akismet": "~0.0.11",
    "async": "~0.1.22",
    "aws-sdk": "~2.230.1",
    "badwords": "~0.0.3",
    "bcryptjs": "~2.4.3",
    "bluebird": "~3.5.0",
    "boolean": "~0.1.3",
    "connect-timeout": "~1.9.0",
    "dotenv": "^16.0.3",
    "express": "~3.21.2",
    "fb": "~1.0.2",
    "http-proxy": "~1.18.1",
    "json2csv": "^6.0.0-alpha.2",
    "lru-cache": "~3.0.0",
    "mimelib": "~0.2.19",
    "morgan": "^1.10.0",
    "nan": "~2.17.0",
    "nodemailer": "~6.4.16",
    "oauth": "~0.9.14",
    "optimist": "~0.3.7",
    "p3p": "~0.0.2",
    "pg": "~8.8.0",
    "pg-connection-string": "~2.5.0",
    "pg-native": "~3.0.1",
    "replacestream": "~4.0.0",
    "request": "~2.88.2",
    "request-promise": "~4.2.6",
    "response-time": "~2.3.1",
    "simple-oauth2": "~0.2.1",
    "sql": "~0.34.0",
    "underscore": "~1.12.1",
    "valid-url": "~1.0.9",
    "winston": "~3.8.2"
  },
  "devDependencies": {
    "@types/async": "~3.2.6",
    "@types/bcrypt": "~3.0.1",
    "@types/bcryptjs": "~2.4.2",
    "@types/bluebird": "~3.5.33",
    "@types/connect-timeout": "~0.0.34",
    "@types/fb": "~0.0.28",
    "@types/http-proxy": "~1.17.5",
    "@types/jest": "^29.4.0",
    "@types/lru-cache": "~5.1.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "~14.14.39",
    "@types/nodemailer": "~6.4.1",
    "@types/nodemailer-mailgun-transport": "~1.4.2",
    "@types/oauth": "~0.9.1",
    "@types/pg": "~7.14.11",
    "@types/replacestream": "~4.0.0",
    "@types/request-promise": "4.1.48",
    "@types/response-time": "~2.3.4",
    "@types/supertest": "^2.0.12",
    "@types/underscore": "~1.11.1",
    "@types/valid-url": "~1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.1",
    "nodemon": "~2.0.20",
    "prettier": "~2.2.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "~4.3.5"
